<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= csrf_meta_tag %>
  <%= stylesheet_link_tag :all %>
</head>
<body>
  <div class="container">
    <%= render 'shared/header' %>
    <%= render 'shared/left' %>
    <div id="right">
      <%= yield %>
    </div>
    <%= render 'shared/footer' %>
    <%= render 'shared/disclaimer' %>
  </div>
  <%= javascript_include_tag :defaults %>
  <%= yield :javascript %>
</body>
</html>
